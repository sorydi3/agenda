package agendaproduccio.utils.grafdispersio;

import java.awt.Color;

import javax.swing.border.MatteBorder;

public class MatteBorderModificable extends MatteBorder
{
	public MatteBorderModificable( int p_top, int p_left, int p_bottom, int p_right, Color p_matteColor )
	{
		super( p_top, p_left, p_bottom, p_right, p_matteColor );		
	}
	
	public void Modificar( int p_top, int p_left, int p_bottom, int p_right, Color p_matteColor )
	{
		top = p_top;
		left = p_left;
		bottom = p_bottom;
		right = p_right;
		color = p_matteColor;
	}
}